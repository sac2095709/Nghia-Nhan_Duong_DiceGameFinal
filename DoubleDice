package dicegamefinal;

/**
 *
 * @author KenOne
 */
public class DoubleDice extends Dice {
    
    public DoubleDice() 
    {
        super(2);
    }
    
@Override
public String toString() 
{
    return "DoubleDice " + super.toString();
}

}

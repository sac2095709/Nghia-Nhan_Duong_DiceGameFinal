package dicegamefinal;

/**
 *
 * @author KenOne
 */
/**
 * The Class DoubleDice Implement MultiThread.
 */
public class DoubleDiceImplementsRunnable {

	/**
	 * The main method.
	 *
	 * @param args the arguments
	 */
	public static void main(String[] args) {

		DoubleDice thread1 = new DoubleDice("Thread 1");
		DoubleDice thread2 = new DoubleDice("Thread 2");
		new Thread(thread1).start();
		new Thread(thread2).start();
		
	}	

}

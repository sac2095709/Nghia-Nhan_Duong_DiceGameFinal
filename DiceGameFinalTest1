package dicegamefinal;

/**
 *
 * @author KenOne
 */
public class DiceGameFinal {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        Dice dx = new Dice();
        DoubleDice ddx = new DoubleDice();
        DoubleDice2 ddx2 = new DoubleDice2(36);
        for (int i = 0; i < 36; i++) {
            dx.Throw();
            ddx.Throw();
            ddx2.Throw();
        }
        System.out.println(dx);
        System.out.println(ddx);
        System.out.println(ddx2);


    }
}
